<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="urn:oasis:names:tc:xliff:document:1.2">
    <attr format="reference" name="numberPickerDownButtonStyle"/>
    <attr format="reference" name="numberPickerInputTextStyle"/>
    <attr format="reference" name="numberPickerStyle"/>
    <attr format="reference" name="numberPickerUpButtonStyle"/>
    <declare-styleable name="NumberPicker"><attr format="color|reference" name="solidColor"/><attr format="boolean" name="flingable"/><attr format="reference" name="selectionDivider"/><attr format="dimension" name="selectionDividerHeight"/><attr format="dimension" name="selectionDividersDistance"/><attr name="android:minHeight"/><attr name="android:maxHeight"/><attr name="android:minWidth"/><attr name="android:maxWidth"/><attr name="android:orientation"/><attr name="android:fadingEdgeLength"/></declare-styleable>
    <integer name="np_config_longAnimTime">500</integer>
    <string name="app_name">Android_Holo_DateTimePicker</string>
    <string name="np_number_picker_decrement_button">Decrement</string>
    <string name="np_number_picker_increment_button">Increment</string>
    <string name="np_number_picker_increment_scroll_action">Slide up to increment and down to decrement.</string>
    <string name="np_number_picker_increment_scroll_mode"><ns1:g example="3" id="value">%s</ns1:g> tap and hold.</string>
    <style name="AiTheme" parent="android:Theme">
        <item name="numberPickerUpButtonStyle">@style/AiWidget.ImageButton.NumberPickerUpButton</item>
        <item name="numberPickerDownButtonStyle">@style/AiWidget.ImageButton.NumberPickerDownButton</item>
        <item name="numberPickerInputTextStyle">@style/AiWidget.EditText.NumberPickerInputText</item>
        <item name="numberPickerStyle">@style/AiWidget.NumberPicker</item>
    </style>
    <style name="AiTheme.Light" parent="android:Theme.Light">
        <item name="numberPickerUpButtonStyle">@style/AiWidget.ImageButton.NumberPickerUpButton.Light</item>
        <item name="numberPickerDownButtonStyle">@style/AiWidget.ImageButton.NumberPickerDownButton.Light</item>
        <item name="numberPickerInputTextStyle">@style/AiWidget.EditText.NumberPickerInputText.Light</item>
        <item name="numberPickerStyle">@style/AiWidget.NumberPicker</item>
    </style>
    <style name="AiThemeSample">
        <item name="numberPickerUpButtonStyle">@style/AiWidget.ImageButton.NumberPickerUpButton</item>
        <item name="numberPickerDownButtonStyle">@style/AiWidget.ImageButton.NumberPickerDownButton</item>
        <item name="numberPickerInputTextStyle">@style/AiWidget.EditText.NumberPickerInputText</item>
        <item name="numberPickerStyle">@style/AiWidget.NumberPicker</item>
    </style>
    <style name="AiThemeSample.Light">
        <item name="numberPickerUpButtonStyle">@style/AiWidget.ImageButton.NumberPickerUpButton.Light</item>
        <item name="numberPickerDownButtonStyle">@style/AiWidget.ImageButton.NumberPickerDownButton.Light</item>
        <item name="numberPickerInputTextStyle">@style/AiWidget.EditText.NumberPickerInputText.Light</item>
        <item name="numberPickerStyle">@style/AiWidget.NumberPicker</item>
    </style>
    <style name="AiWidget">
        <item name="android:textAppearance">?android:attr/textAppearance</item>
    </style>
    <style name="AiWidget.EditText">
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:clickable">true</item>
        <item name="android:textAppearance">?android:attr/textAppearanceMediumInverse</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="AiWidget.EditText.NumberPickerInputText">
        <item name="android:paddingTop">13sp</item>
        <item name="android:paddingBottom">13sp</item>
        <item name="android:paddingLeft">2sp</item>
        <item name="android:paddingRight">2sp</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@null</item>
    </style>
    <style name="AiWidget.EditText.NumberPickerInputText.Light" parent="AiWidget.EditText.NumberPickerInputText"/>
    <style name="AiWidget.ImageButton">
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:scaleType">center</item>
        <item name="android:background">@android:drawable/btn_default</item>
    </style>
    <style name="AiWidget.ImageButton.NumberPickerDownButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/np_numberpicker_down_btn_holo_dark</item>
        <item name="android:paddingTop">22dip</item>
        <item name="android:paddingBottom">16dip</item>
    </style>
    <style name="AiWidget.ImageButton.NumberPickerDownButton.Light" parent="AiWidget.ImageButton.NumberPickerDownButton">
        <item name="android:src">@drawable/np_numberpicker_down_btn_holo_light</item>
    </style>
    <style name="AiWidget.ImageButton.NumberPickerUpButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/np_numberpicker_up_btn_holo_dark</item>
        <item name="android:paddingTop">16dip</item>
        <item name="android:paddingBottom">22dip</item>
    </style>
    <style name="AiWidget.ImageButton.NumberPickerUpButton.Light" parent="AiWidget.ImageButton.NumberPickerUpButton">
        <item name="android:src">@drawable/np_numberpicker_up_btn_holo_light</item>
    </style>
    <style name="AiWidget.Light.NumberPicker" parent="AiWidget.NumberPicker"/>
    <style name="AiWidget.NumberPicker">
        <item name="android:orientation">vertical</item>
        <item name="android:fadingEdge">vertical</item>
        <item name="android:fadingEdgeLength">50dip</item>
        <item name="solidColor">@android:color/transparent</item>
        <item name="flingable">true</item>
        <item name="selectionDivider">@drawable/dialog_set_time_divider</item>
        <item name="selectionDividerHeight">2dip</item>
        <item name="android:minWidth">48dip</item>
        <item name="android:maxHeight">200dip</item>
    </style>
    <style name="AppTheme" parent="android:Theme.Light"/>
</resources>